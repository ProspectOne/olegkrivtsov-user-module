{
    "name": "prospect-one/user-module",
    "description": "UserModule patched specifically for the ProspectOne",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ext-gd": "*",
        "ext-intl": "*",
        "zendframework/zend-component-installer": "2.1.1",
        "zendframework/zend-mvc": "3.1.1",
        "zfcampus/zf-development-mode": "3.2.0",
        "zendframework/zend-mvc-form": "1.0.0",
        "zendframework/zend-mvc-plugins": "1.0.1",
        "zendframework/zend-session": "2.8.5",
        "zendframework/zend-authentication": "2.6.0",
        "doctrine/doctrine-orm-module": "2.1.4",
        "doctrine/doctrine-module": "2.1.8",
        "zendframework/zend-math": "3.2.0",
        "zendframework/zend-crypt": "3.3.0",
        "zendframework/zend-captcha": "2.8.0"
    },
    "autoload": {
        "psr-4": {
            "ProspectOne\\UserModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProspectOne\\UserModuleTest\\": "test/"
        }
    },
    "extra": [],
    "scripts": {
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php"
    }
}
